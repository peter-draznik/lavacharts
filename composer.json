{
    "name": "peter-draznik/lavacharts",
    "description": "PHP wrapper library for the Google Chart API",
    "keywords": [
        "charts",
        "graphs",
        "google"
    ],
    "homepage": "http://lavacharts.com",
    "license": "MIT",
    "authors": [
        {
            "name": "Kevin Hill",
            "email": "kevinkhill@gmail.com",
            "role": "Developer"
        },
        {
            "name": "Peter Draznik",
            "email": "Peter.Draznik@38thStreetStudios.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=5.4.0",
        "nesbot/carbon": "~1.14"
    },
    "suggest": {
        "khill/datatableplus": "Adds extra features to DataTables such as csv and laravel collection parsing."
    },
    "autoload": {
        "psr-4": {
            "Khill\\Lavacharts\\": "src/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "~4.5",
        "mybuilder/phpunit-accelerator": "~1.1",
        "mockery/mockery": "~0.9",
        "squizlabs/php_codesniffer": "~2.3",
        "sami/sami": "~3.0",
        "satooshi/php-coveralls": "dev-master"
    },
    "autoload-dev": {
        "psr-4": {
            "Khill\\Lavacharts\\Tests\\": "tests/"
        }
    },
    "minimum-stability": "stable"
}
